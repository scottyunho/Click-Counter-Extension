{
  "name": "Getting Started Example",
  "version": "1.0",
  "description": "Build an Extension!",
  "permissions": ["storage"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "manifest_version": 2,

  "browser_action": {
    "default_popup": "popup.html",
    "scripts": ["popup.js"],
    "default_title": "Clicks"
  }

}